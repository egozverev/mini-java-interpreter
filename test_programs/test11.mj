class Factorial {
  public static void main () {
    Fac x;
    x = new Fac();
    System.out.println (x.ComputeFac(10));
  }
}

class Fac {
  public int ComputeFac (int num) {
    int num_aux;
    if (num == 0) {
      num_aux = 1;
    }
    else {
      num_aux = num * this.ComputeFac (num-1);
    }
    return num_aux;
  }
}