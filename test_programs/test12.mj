class Factorial {
  public static void main () {
    Fac x;
    x = new Fac();
    System.out.println (x.ComputeFac(10));
  }
}

class Fac {
  public int ComputeFac (int num) {
    int i;
    i = 2;
    cur = 1;
    while (i < num + 1) {
        cur = cur * i;
        i = i + 1;
    }
    return cur;
  }
  int cur;
}